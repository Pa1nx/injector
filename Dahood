local UserNameHere = "Ronald09592DT"
local function ClickLoop()
    while true do
        wait(0.1) -- Wait for a short period before checking again
        local player = game.Players.LocalPlayer
        local hrp = player.Character and player.Character:WaitForChild("HumanoidRootPart")
        local targetPlayer = game.Players:WaitForChild(UserNameHere)

        if hrp and targetPlayer then
            local targetHrp = targetPlayer.Character and targetPlayer.Character:WaitForChild("HumanoidRootPart")
            if targetHrp then
                hrp.CFrame = targetHrp.CFrame + Vector3.new(0, 10, 0)
                
            else
                warn("Target player's HumanoidRootPart not found.")
            end
        else
            warn("Local player or target player not found.")
        end
    end
end


coroutine.wrap(ClickLoop)()
wait(2)
