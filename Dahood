while true do
    wait(0.01)

local player = game.Players.LocalPlayer
local hrp = player.Character and player.Character:WaitForChild("HumanoidRootPart")
local targetPlayer = game.Players:WaitForChild(UserNameHere)
    if targetPlayer and targetPlayer.Character and targetPlayer.Character:WaitForChild("HumanoidRootPart") then
        local distance = (targetPlayer.Character.HumanoidRootPart.Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).magnitude
        if distance <= 20 then
            local args = {
                [1] = "DropMoney",
                [2] = "10000"
            }
            
            game:GetService("ReplicatedStorage").MainEvent:FireServer(unpack(args))
        else
         local args = {
    [1] = "The bot stops dropping when not in radius, u cant get, just stop.",
    [2] = "All"
}

game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer(unpack(args))
        end
    else
        local args = {
    [1] = "The bot stops dropping when not in radius, u cant get, just stop.",
    [2] = "All"
}

game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer(unpack(args))
    end
end


